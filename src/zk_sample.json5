{
    $schema:'https://static.choyunjin.kr/codes/schema/json/zk/zk0.json', // 스키마 선언
    midi:{
        /**
        헤더...라고는 했지만 사실
        여기 들어갈건 division밖에 없음
        divtype이 tpb면
        div0에 박자당 틱이 와야하고
        (이때는 div1이 필요없음),
        divtype이 smtpe면
        div0에 초당 프레임수,
        div1에 프레임당 틱수가 와야한다.
        */
        header:{
            divtype:'tpb',
            div0:'120'
        },
        
        /*
        공통으로 들어가야 하는 meta 이벤트
        (주로 재생시간이나 파일 자체에 관련된 것들)
        여기에 와야 하는 이벤트:
        lyrics(5)
        set tempo(81)
        smtpe offset(84)
        time signature(88)
        key signature(89)
        구조는 밑에 data부분 참조
        */
        global:{
            meta:{
                copyright_notice:'(C) 2021~ choyunjin'
            },
            events:[{
                dt:0,t:255,st:81,
                d:{
                    type:'bpm',
                    tempo:150
                }
            },{
                dt:7800,t:255,st:81,
                d:{
                    type:'microsec',
                    tempo:250000
                }
            }]
        },
        
        // 나머지 이벤트
        data:[
            // 배열 1개 = 포트 1개
            // 배열 안에 객체 1개 = 트랙 1개(global과 같은 구조)
            [{
                meta:{
                    track_name:'Track 1-1'
                },
                
                /*
                dt는 delta time, t는 type, st는 subtype
                event type은 type과 subtype으로 나뉨(둘다 숫자)
                type: meta(255),midi(8),sysex(240),escape(247)으로 총 4개
                subtype: meta는 meta대로 midi는 midi대로 겁나 많음
                (sysex,escape같이 subtype이 필요 없으면
                그냥 안넣거나 0을 넣으면 됨) 
                */
                events:[{
                    // meta 이벤트
                    dt:0,t:255,st:0,
                    d:{ // 객체 d를 쓰는 경우(d는 data)
                        // sequence number(0) 일 따 들어가는 것들
                        msb:0, // msb
                        lsb:0, // lsb
                        
                        /*
                        text(1),lyrics(5)
                        marker(6),cue point(7)
                        일 때 들어가는 것들
                        */
                        txt:'...내용...',
                        
                        // midi channel prefix(32) 일 때 들어가는 것들
                        prefix:0,
                        
                        // end of track(47) 은 데이터가 필요 없음
                        
                        // set tempo(81) 일 때 들어가는 것들
                        type:'bpm', // bpm이면 bpm,microsec이면 박자당 마이크로초
                        tempo:150,
                        
                        // key signature(89) 일 때 들어가는 것들
                        key:0, // 음정. 도(C)가 0이고 -7~7까지 지정가능
                        min:false // true=단조,false=장조
                    },
                    // 배열 bd를 쓰는 경우(bd는 byte data)
                    bd:[
                        // smtpe offset(84): 순서대로 시간,분,초,프레임,서브프레임(?)
                        // time signature(88): midi 파일 표준과 동일
                        // sequencer specific(127): midi 파일 표준과 동일
                    ]
                },{
                    /*
                    midi 이벤트
                    subtype 종류:
                    note off(8)
                    note on(9)
                    note aftertouch(10)
                    control change(11)
                    program change(12)
                    channel aftertouch(13)
                    pitch bend(14)
                    */
                    dt:360,t:8,st:0,
                    d:{
                        // c는 channel(0부터 시작)
                        // p1은 param2, p2는 param2
                        // p2가 필요없으면 안넣거나 0을 넣으면 됨
                        c:0,p1:0,p2:0
                    }
                },{
                    // sysex 이벤트
                    // 내용물은 맨 앞에 0xf0(240)과
                    // 맨 뒤에 0xf7(247) 을 포함하지 않음
                    dt:360,t:240,
                    bd:[19,72,11,21,19,72,...]
                },{
                    // escape 이벤트
                    // 내용물을 그대로 midi 신호로 보냄
                    dt:360,t:247,
                    bd:[19,72,11,21,19,72,...]
                }]
            },{
                meta:{
                    // ...
                },
                events:[
                    // ...
                ]
            }{
                meta:{
                    // ...
                },
                events:[
                    // ...
                ]
            }{
                meta:{
                    // ...
                },
                events:[
                    // ...
                ]
            }]
        ]
    }
}